{
  "name": "rust-kgdb",
  "version": "0.8.14",
  "description": "High-performance RDF/SPARQL database with AI agent framework and cross-database federation. GraphDB (449ns lookups, 5-11x faster than RDFox), HyperFederate (KGDB + Snowflake + BigQuery), GraphFrames analytics, Datalog reasoning, HNSW vector embeddings. HyperMindAgent for schema-aware query generation with audit trails. W3C SPARQL 1.1 compliant. Native performance via Rust + NAPI-RS.",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "rust-kgdb-napi",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu"
      ]
    }
  },
  "scripts": {
    "build": "napi build --platform --release native/rust-kgdb-napi",
    "build:debug": "napi build --platform native/rust-kgdb-napi",
    "test": "node test-all-features.js",
    "test:jest": "jest"
  },
  "keywords": [
    "neuro-symbolic-ai",
    "agentic-framework",
    "schema-aware-graphdb",
    "context-theory",
    "bring-your-own-ontology",
    "schema-caching",
    "llm-planner",
    "proof-dag",
    "curry-howard",
    "memory-hypergraph",
    "agent-memory",
    "temporal-memory",
    "category-theory",
    "type-theory",
    "llm-agents",
    "wasm-sandbox",
    "morphism-composition",
    "rdf",
    "sparql",
    "knowledge-graph",
    "lubm-benchmark",
    "claude-ai",
    "gpt-4o",
    "secure-execution",
    "capability-based-security",
    "graphframes",
    "datalog",
    "reasoning",
    "napi-rs",
    "rust",
    "fraud-detection",
    "underwriting"
  ],
  "author": "Gonnect Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gonnect-uk/rust-kgdb"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">= 14"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "hypermind-agent.js",
    "secure-agent-sandbox-demo.js",
    "vanilla-vs-hypermind-benchmark.js",
    "benchmark-frameworks.py",
    "verified_benchmark_results.json",
    "examples/",
    "ontology/",
    "README.md",
    "CLAUDE.md",
    "DESIGN.md",
    "IMPLEMENTATION_GUIDE.md",
    "HYPERMIND_BENCHMARK_REPORT.md",
    "CHANGELOG.md",
    "*.node"
  ]
}
