{
  "name": "rust-kgdb",
  "version": "0.1.3",
  "description": "High-performance RDF/SPARQL database with 100% W3C compliance",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "rust-kgdb-napi",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu"
      ]
    }
  },
  "scripts": {
    "build": "napi build --platform --release native/rust-kgdb-napi",
    "build:debug": "napi build --platform native/rust-kgdb-napi",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "jest",
    "version": "napi version"
  },
  "keywords": [
    "rdf",
    "sparql",
    "semantic-web",
    "knowledge-graph",
    "database",
    "triplestore",
    "napi-rs",
    "rust"
  ],
  "author": "Gonnect Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/zenya/rust-kgdb"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">= 14"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "*.node"
  ]
}
