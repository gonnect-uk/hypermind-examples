@prefix av: <http://zenya.com/ontology/av#> .
@prefix sensor: <http://zenya.com/ontology/sensor#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .

# Scenario 3: School Zone Speed Limit Violation
# Vehicle traveling at 54 km/h (15 m/s) in 30 km/h school zone

# Ego Vehicle
<http://zenya.com/vehicle/ego>
    a av:Vehicle ;
    rdfs:label "Ego Vehicle" ;
    av:hasVelocity "15.0"^^xsd:float ;  # m/s (54 km/h)
    av:positionX "-50.0"^^xsd:float ;   # meters
    av:carLength "5.0"^^xsd:float ;     # meters
    geo:lat "51.5074"^^xsd:float ;
    geo:long "-0.1278"^^xsd:float .

# School Zone Sign
<http://zenya.com/sign/school_zone_001>
    a av:SchoolZoneSign ;
    rdfs:label "School Zone Sign" ;
    av:positionX "-80.0"^^xsd:float ;   # meters
    av:speedLimit "8.33"^^xsd:float ;   # m/s (30 km/h)
    av:speedLimitKmh "30"^^xsd:integer ;
    av:zoneType "school"^^xsd:string ;
    av:activeHours "07:00-17:00"^^xsd:string .

# School Zone Area
<http://zenya.com/zone/school_001>
    a av:SchoolZone ;
    rdfs:label "Primary School Zone" ;
    av:hasSign <http://zenya.com/sign/school_zone_001> ;
    av:startPosition "-80.0"^^xsd:float ;
    av:endPosition "80.0"^^xsd:float ;
    av:speedLimit "8.33"^^xsd:float ;   # m/s (30 km/h)
    av:isActive "true"^^xsd:boolean ;
    av:currentTime "14:30"^^xsd:string . # Within active hours

# Camera Sensor (Sign Detection)
<http://zenya.com/sensor/camera_front>
    a sensor:CameraSensor ;
    rdfs:label "Front Camera" ;
    sensor:detects <http://zenya.com/sign/school_zone_001> ;
    sensor:confidence "0.97"^^xsd:float ;
    sensor:timestamp "2025-11-27T14:30:00Z"^^xsd:dateTime ;
    sensor:signType "school_zone"^^xsd:string .

# GPS Sensor
<http://zenya.com/sensor/gps>
    a sensor:GPSSensor ;
    rdfs:label "GPS Sensor" ;
    sensor:accuracy "2.5"^^xsd:float ;  # meters
    geo:lat "51.5074"^^xsd:float ;
    geo:long "-0.1278"^^xsd:float ;
    sensor:timestamp "2025-11-27T14:30:00Z"^^xsd:dateTime .

# Speed Violation
<http://zenya.com/violation/speed_001>
    a av:SpeedViolation ;
    av:subject <http://zenya.com/vehicle/ego> ;
    av:location <http://zenya.com/zone/school_001> ;
    av:currentSpeed "15.0"^^xsd:float ;    # m/s (54 km/h)
    av:currentSpeedKmh "54"^^xsd:integer ;
    av:limitSpeed "8.33"^^xsd:float ;      # m/s (30 km/h)
    av:limitSpeedKmh "30"^^xsd:integer ;
    av:excessSpeed "6.67"^^xsd:float ;     # m/s (24 km/h over)
    av:excessSpeedKmh "24"^^xsd:integer ;
    av:violationSeverity "high"^^xsd:string ;
    av:requiresAction "immediate_deceleration"^^xsd:string .

# Safety Standard
<http://zenya.com/standard/traffic_law>
    a av:TrafficRegulation ;
    rdfs:label "School Zone Traffic Law" ;
    av:requiresSpeedReduction "true"^^xsd:boolean ;
    av:minConfidence "0.85"^^xsd:float ;
    av:penaltyMultiplier "2.0"^^xsd:float ; # Double penalty in school zones
    av:vulnerablePopulation "children"^^xsd:string .

# Target Deceleration
<http://zenya.com/physics/deceleration>
    a av:PhysicsCalculation ;
    av:requiredDeceleration "3.5"^^xsd:float ;  # m/sÂ²
    av:targetSpeed "8.33"^^xsd:float ;          # m/s (30 km/h)
    av:decelerationDistance "20.0"^^xsd:float ; # meters
    av:timeToTarget "1.9"^^xsd:float .          # seconds
