# === Compatibility Rules (Ontology) ===
compat:CompatibilityRule rdf:type rdfs:Class ;
    rdfs:label "Compatibility Rule"@en ;
    rdfs:comment "Defines product compatibility constraints"@en .

compat:requiresSocket rdf:type rdf:Property ;
    rdfs:domain gr:ProductOrService ;
    rdfs:range xsd:string ;
    rdfs:comment "CPU socket requirement"@en .

compat:compatibleWithCPU rdf:type rdf:Property ;
    rdfs:domain gr:ProductOrService ;
    rdfs:range gr:ProductOrService ;
    rdfs:comment "Compatible CPU products"@en .

compat:minPowerSupply rdf:type rdf:Property ;
    rdfs:domain gr:ProductOrService ;
    rdfs:range xsd:integer ;
    rdfs:comment "Minimum power supply wattage required"@en .
