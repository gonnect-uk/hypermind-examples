# -----------------------------------------------------------------------------
# SAMPLE QUERIES (Example SPARQL Queries)
# -----------------------------------------------------------------------------

:Query_AllPolicies a db:SPARQLQuery ;
    rdfs:label "Get All Policies" ;
    db:queryId "Q001" ;
    db:sparql """SELECT ?policy ?holder ?risk WHERE {
    ?policy a ins:Policy .
    ?policy ins:policyHolder ?holderRef .
    ?holderRef rdfs:label ?holder .
    ?policy ins:riskLevel ?risk .
}""" ;
    db:avgExecutionTime "15.2"^^xsd:float ;
    db:executionTimeUnit "µs" ;
    db:expectedResults 5 .

:Query_HighRiskTransactions a db:SPARQLQuery ;
    rdfs:label "High Risk Transactions" ;
    db:queryId "Q002" ;
    db:sparql """SELECT ?txn ?amount ?risk WHERE {
    ?txn a fin:Transaction .
    ?txn fin:amount ?amount .
    ?txn fin:riskScore ?risk .
    FILTER (?risk > 0.7)
}""" ;
    db:avgExecutionTime "8.5"^^xsd:float ;
    db:executionTimeUnit "µs" ;
    db:expectedResults 2 .

:Query_ProductInventory a db:SPARQLQuery ;
    rdfs:label "Product Inventory Status" ;
    db:queryId "Q003" ;
    db:sparql """SELECT ?product ?name ?stock WHERE {
    ?product a schema:Product .
    ?product schema:name ?name .
    ?product prod:stockQuantity ?stock .
    FILTER (?stock < 20)
}""" ;
    db:avgExecutionTime "12.3"^^xsd:float ;
    db:executionTimeUnit "µs" ;
    db:expectedResults 3 .

